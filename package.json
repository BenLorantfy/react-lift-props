{
  "name": "react-lift-props",
  "version": "1.1.6",
  "description": "Let's you create a system of deeply nested components",
  "scripts": {
    "start": "start-storybook -p 6006",
    "setup": "rm -Rf node_modules && npm install && npm run move-enzyme-types",
    "move-enzyme-types": "mv ./node_modules/@benlorantfy/enzyme-types ./node_modules/@types/benlorantfy__enzyme || true",
    "build": "babel src -d lib --ignore **/__tests__/**,**/_stories.tsx --extensions \".js,.jsx,.ts,.tsx\"",
    "build:watch": "babel src -w -d lib --ignore **/__tests__/**,**/_stories.tsx --extensions \".js,.jsx,.ts,.tsx\"",
    "clean": "rm -Rf lib",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watchAll --coverage",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "tslint 'src/**/*.{ts,tsx}' --fix",
    "type-check": "tsc --noEmit",
    "prepublish": "npm run clean && npm run build"
  },
  "files": [
    "lib/**",
    "README.md",
    "index.js"
  ],
  "main": "index.js",
  "author": "Ben Lorantfy <ben@lorantfy.com>",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/BenLorantfy/react-lift-props"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "@babel/preset-stage-0": "^7.0.0-beta.44",
    "@babel/preset-typescript": "^7.0.0-beta.44",
    "@benlorantfy/enzyme": "^3.3.0",
    "@benlorantfy/enzyme-adapter-react-16": "^1.1.4",
    "@benlorantfy/enzyme-adapter-utils": "^1.3.0",
    "@benlorantfy/enzyme-types": "^3.3.0",
    "@storybook/addon-actions": "^3.4.2",
    "@storybook/addon-links": "^3.4.2",
    "@storybook/react": "^3.4.2",
    "@types/enzyme": "^3.1.10",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.12",
    "@types/storybook__react": "^3.0.7",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.4.3",
    "jest": "^22.4.3",
    "raf": "^3.4.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  }
}
